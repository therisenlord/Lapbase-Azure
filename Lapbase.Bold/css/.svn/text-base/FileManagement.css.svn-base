/* Sub Menu */	
	
.subContentMnu {
	margin: 0;
	padding: 5px 0 0 6px;
	height: 35px;
	}
.subContentMnu ul {
	border: 0;
	margin: 0;
	padding: 0;
	list-style-type: none;
	text-align: center;
	}
.subContentMnu ul li {
	display: block;
	float: left;
	text-align: center;
	padding: 0;
	margin: 0;
	}
.subContentMnu ul li a {
	height: 30px;
	width: 105px;
	padding: 0;
	margin: 0;
	text-decoration: none;
	text-indent: -1000em;
	/*overflow: hidden;*/
	display: block;
	}
/*Hide from Mac IE\*/
.subContentMnu ul li a {
	overflow: hidden;
	}
/*End hide*/

.subContentMnu ul li a:hover {
	background-position: bottom left;
	}
.subContentMnu a:active {
	color: #fff;
	}
.subContentMnu li#active a {
	color: #fff;
	}
	
