/* New CSS for FileManagement */
	
.mediaMangerWrap {
	padding: 5px;
	background-color: #efefef;
	}
.leftColumn {
	margin: 0;
	padding: 0;
	float: left;
	width: 262px;
	}
.rightColumn {
	margin: 0;
	padding: 0;
	float: right;
	width: 644px;
	background: transparent url(../img/main_panel_bg.gif) repeat-y top left;
	}
.rightColumn div.boxTop {
	margin: 0;
	padding: 1px 0 0 0;
	background: transparent url(../img/main_panel_top.gif) no-repeat top left;
	}
.rightColumn div.boxBtm {
	clear: both;
	height: 5px;
	background: transparent url(../img/main_panel_btm.gif) no-repeat bottom left;
	}	
	
.imgDisplay {
	padding: 0 0 0 15px;
	}
.imgWrap {
	margin: 0;
	padding: 5px 10px 0 0;
	float: left;
	width: 89px;
	text-align: center;
	font-size: 0.9em;
	}
.imgWrap p {
	margin: 3px 0 0 0;
	padding: 0;
	}
.imgWrap a {
	padding: 1px 3px;
	text-decoration: none;
	color: #666;
	}
.imgWrap a:hover {
	text-decoration: none;
	background-color: #ccc;
	}
.imgShadow {
	width: 88px;
	border-right: 1px solid #ededed;
	border-bottom: 1px solid #ededed;
	}
.imgShadow img {
	border: 1px solid #c2c2c2;
	}
.txtWrap {
	margin: 0;
	padding: 0;
	float: left;
	width: 475px;
	}
.fileDetailsHistory {
	padding: 15px;
	}
.operations {
	padding: 0 10px;
	}
.operationDetails {
	padding: 5px;
	background-color: #fff9d9;
	border: 1px solid #D7C88D;
	border-top: 0;
	border-left: 0;
	}
.operationDetails p {
	margin: 0 0 10px 0;
	padding: 0;
	}
.operationDetails p img {
	margin: 0;
	padding: 0 5px 0 0;
	display: block;
	float: left;
	}
.operationDetails p span {
	margin: 0;
	padding: 0 0 3px 0;
	display: block;
	float: left;
	font-weight: bold;
	}
.operationDetails a {
	padding: 1px 3px;
	text-decoration: none;
	color: #333;
	}
.operationDetails a:hover {
	text-decoration: underline;
	}
.operationDetails table {
	clear: both;
	}
	
	
.sortFiles {
	margin: 0 0 5px 0;
	padding: 0;
	background: transparent url(../img/sort_panel_bg.gif) repeat-y top left;
	}
.sortFiles div.boxTop {
	margin: 0;
	padding: 1px 0 0 0;
	background: transparent url(../img/sort_panel_top.gif) no-repeat top left;
	}
.sortFiles div.boxBtm {
	clear: both;
	height: 5px;
	background: transparent url(../img/sort_panel_btm.gif) no-repeat bottom left;
	}	
.sortFiles h3 {
	padding: 0 0 0 5px;
	margin: 6px 0 10px 0;
	}	
	
